{{#form_for exchange "/admin/account" }}


    {{#if @f}}
        <div class="error">
            {{display @f}}
        </div>
    {{/if}}

    <div>
        <label>Username:</label>
        {{ tag_input @f "user_name" value=@d.user_name }}
    </div>

    <div>
        <p><label>Email:</label></p>
        {{ tag_input @f "email" type="email" value=@d.email }}
    </div>

    <div>
        <input type="submit" value="Save">
    </div>
{{/form_for}}
<hr>



